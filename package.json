{
  "name": "contract-tracing-graphql-api",
  "version": "1.0.0",
  "description": "Express+Apollo Server",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc",
    "lint": "./node_modules/.bin/eslint --ext ts ./src",
    "setup": "npm run lint && npm run build",
    "start": "npm run setup && node ./build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w",
    "dev": "yarn run watch && yarn run start"
  },
  "dependencies": {
    "apollo-server": "^2.11.0",
    "apollo-server-express": "^2.11.0",
    "dotenv": "^8.2.0",
    "helmet": "^3.21.3",
    "neo4j-driver": "^4.0.2",
    "neo4j-graphql-js": "^2.13.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.45",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "boilerplate",
    "graphql"
  ],
  "author": "Darshan Shah",
  "license": "ISC"
}
